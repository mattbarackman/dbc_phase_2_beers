<div class="container">

  <% if current_user %>

    <h1> Welcome <%=current_user.name%>! </h1>
    <div class="users">
      <p> Users: </p>
      <ul>
        <% @users.each do |user| %>
          <li> <%=user.name%> </li>
        <% end %>          
      </ul>
    </div>
  
  <% else %>
  
    <h1> Welcome! </h1>
  
  <% end %>
</div>
